# VSTS Frontend Production Environment

# API Configuration
VITE_API_URL=https://api.vsts.company.com
VITE_API_TIMEOUT=10000

# Supabase Configuration (Production)
VITE_SUPABASE_URL=https://your-production-supabase.supabase.co
VITE_SUPABASE_ANON_KEY=production_supabase_anon_key

# Application Configuration
VITE_APP_NAME=VSTS
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Visitor and Staff Tracking System

# Feature Flags
VITE_ENABLE_EMERGENCY_MODE=true
VITE_ENABLE_KIOSK_MODE=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true

# Development/Debug Configuration
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error

# Build Configuration (Add your actual values)
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# Kiosk Configuration
VITE_KIOSK_AUTO_LOGOUT_TIMEOUT=180000
VITE_KIOSK_IDLE_TIMEOUT=30000