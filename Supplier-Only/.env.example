# Build Configuration
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# Kiosk Configuration
VITE_KIOSK_AUTO_LOGOUT_TIMEOUT=300000
VITE_KIOSK_IDLE_TIMEOUT=60000